CREATE TABLE [{{ schema.name }}].[{{ table.name }}](
{%- for column in table.columns %}
    {% if not loop.first %},{% endif %}[{{ column.name }}] {{ column.data_type -}}
{% if not (column.data_type_precision is none or column.data_type == "INT") -%}
({{ column.data_type_precision }}, {{ column.data_type_numeric_scale }})
{%- elif not (column.data_type_length is none) -%}
({{ column.data_type_length }})
{%- endif -%}
{% endfor %}
);
