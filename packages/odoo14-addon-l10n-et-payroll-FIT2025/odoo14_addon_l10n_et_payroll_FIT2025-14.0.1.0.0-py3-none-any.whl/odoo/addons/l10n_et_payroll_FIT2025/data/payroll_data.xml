    <record id="register_FITA" model="hr.contribution.register">
        <field
            name="name"
        >Federal Income Tax Withholding Register (1395/2017)</field>
        <field name="partner_id" eval="False" />
    </record>

    <record id="hr_payroll_rule_fit" model="hr.salary.rule">
        <field name="code">FIT2017</field>
        <field name="name">Federal Income Tax Withholding</field>
        <field name="category_id" ref="l10n_et_payroll_category.FITA" />
        <field name="condition_select">none</field>
        <field name="appears_on_payslip" eval="False" />
        <field name="register_id" ref="register_FITA" />
        <field name="sequence" eval="3100" />
        <field name="note" />
    </record>

