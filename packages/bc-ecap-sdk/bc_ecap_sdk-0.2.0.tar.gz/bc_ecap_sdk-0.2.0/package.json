{
  "name": "bc-ecap-sdk",
  "version": "0.1.0",
  "description": "",
  "main": "index.node",
  "scripts": {
    "cargo-build": "cargo b --lib --message-format=json-render-diagnostics > cargo.log",
    "cross-build": "cross b --message-format=json-render-diagnostics > cross.log",
    "postcargo-build": "neon dist < cargo.log && sh scripts/copy-node.sh",
    "postcross-build": "neon dist < cargo.log && sh scripts/copy-node.sh",
    "test": "node examples/wasm/ecap/example_eeg_cap_ble.js",
    "dev": "yarn cargo-build --features \"ble eeg-cap node_addons\"",
    "prod": "yarn cargo-build --release --features \"ble eeg-cap node_addons\"",
    "cross": "yarn cross-build --release --features \"ble eeg-cap node_addons\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@neon-rs/cli": "0.1.82"
  }
}
