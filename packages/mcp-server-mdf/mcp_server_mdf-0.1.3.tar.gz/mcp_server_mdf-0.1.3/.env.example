# MDF MCP Server Configuration

# Maximum number of concurrent MDF sessions
MAX_SESSIONS=10

# Session timeout in seconds (1 hour default)
SESSION_TIMEOUT=3600

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Maximum file size in MB for full memory mode
MAX_FILE_SIZE_FULL_MEMORY=500

# Default memory mode for opening files (full, low, minimum)
DEFAULT_MEMORY_MODE=full