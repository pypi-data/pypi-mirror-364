{% set title = gettext("Radio") %}
{% set back_url = "/" %}
{% set script = "radio" %}
{% extends "base_static.jinja2" %}

{% block content %}

<audio id="audio"></audio>

<button id="play">Click to play (or allow autoplay in your browser)</button><br>

<img id="image" height="500" width="500"><br>

Status: <span id="status"></span><br>
Current: <span id="current"></span><br>
Next: <span id="next"></span>

{% endblock content %}
