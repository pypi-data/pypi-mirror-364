msgstr "collective.collabora.collabora_server_url no está configurado."

#. Default: "collective.collabora.collabora_server_url needs to be a full URL."
#: ../browser/edit.py:126
msgid "error_collabora_server_url_invalid"
msgstr "collective.collabora.collabora_server_url debe ser una URL completa."

#. Default: "Collabora does not support mimetype ${mimetype}."
#: ../browser/edit.py:176
msgid "error_editor_mimetype"
msgstr "Collabora no admite el mimetype ${mimetype}."

#. Default: "Cannot extract url source from action"
#: ../browser/edit.py:185
msgid "error_editor_urlsrc"
msgstr "No se puede extraer la fuente URL de la acción"

#. Default: "JWT Authentication Plugin not found. Is plone.restapi installed?"
#: ../browser/edit.py:207
msgid "error_jwt_plugin"
msgstr "No se ha encontrado el plugin de autenticación JWT. ¿Está instalado plone.restapi?"

#. Default: "When Plone is supposed to be accessed on localhost, Collabora cannot call back. Use host.docker.internal or a public FQDN instead."
