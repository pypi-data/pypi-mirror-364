# Basic Service Configuration
TELEMETRY_SERVICE_NAME=shahabas-test
TELEMETRY_SERVICE_VERSION=1.0.0
TELEMETRY_ENVIRONMENT=development

# OpenTelemetry Collector Endpoint
TELEMETRY_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

# Logging Configuration
TELEMETRY_LOG_LEVEL=INFO

# Metrics Configuration
TELEMETRY_METRIC_EXPORT_INTERVAL=5000
TELEMETRY_METRIC_EXPORT_TIMEOUT=4000

# Resource Attributes (optional)
TELEMETRY_RESOURCE_ATTRIBUTES={"team":"backend","owner":"platform-team"}
