{
  "collection_name": "simple_example",
  "description": "Simple example schema for getting started",
  "fields": [
    {
      "name": "id",
      "type": "Int64",
      "is_primary": true,
      "auto_id": true,
      "description": "Primary key"
    },
    {
      "name": "name",
      "type": "VarChar",
      "max_length": 100,
      "description": "Item name"
    },
    {
      "name": "description",
      "type": "VarChar",
      "max_length": 500,
      "description": "Item description"
    },
    {
      "name": "price",
      "type": "Float",
      "min": 0.01,
      "max": 1000.0,
      "description": "Item price"
    },
    {
      "name": "in_stock",
      "type": "Bool",
      "description": "Whether item is in stock"
    },
    {
      "name": "embedding",
      "type": "FloatVector",
      "dim": 128,
      "description": "Feature embedding vector"
    }
  ]
}