{
  "mcpServers": {
    "cloud-mcp": {
      "command": "python",
      "args": ["server.py"],
      "env": {
        "CLOUD_BASE_URL": "http://localhost:8080",
        "CLOUD_USERNAME": "your-email@example.com",
        "CLOUD_PASSWORD": "your-password",
        "CLOUD_TIMEZONE": "Asia/Shanghai",
        "CLOUD_AREA": "CN",
        "CLOUD_COUNTRY": "China"
      }
    }
  }
} 