, 
.br
.br
.BI "    NVME_ST_CODE_SHORT"
, 
.br
.br
.BI "    NVME_ST_CODE_EXTENDED"
, 
.br
.br
.BI "    NVME_ST_CODE_HOST_INIT"
, 
.br
.br
.BI "    NVME_ST_CODE_VS"
, 
.br
