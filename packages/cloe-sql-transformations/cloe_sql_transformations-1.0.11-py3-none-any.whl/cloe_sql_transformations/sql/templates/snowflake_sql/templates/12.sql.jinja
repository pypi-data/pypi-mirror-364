INSERT INTO {{ sink_table_identifier_artifact }}
	(
		BK,
		{{ sink_insert_artifact }}
		{{',' ~ lu_sink_insert_artifact if lu_sink_insert_artifact else '' }},
		_DCR,
		_DMR,
		_IsDeleted,
		_Deleted_Date
	)
	SELECT
		{{ bk_artifact }},
		{{ source_insert_artifact }}
		{{',' ~ lu_source_insert_artifact if lu_source_insert_artifact else '' }},
		CURRENT_TIMESTAMP(),
		CURRENT_TIMESTAMP(),
		0,
		NULL
	FROM {{ source_table_identifier_artifact }} s
	LEFT JOIN {{ sink_table_identifier_artifact }} t ON t.BK = {{ bk_artifact }} {{ lu_sink_source_join_artifact if lu_sink_source_join_artifact else '' }}
	WHERE t.BK IS NULL
	{%- if include_dq2 -%}
	AND s._isConvertError = 0
	{%- elif include_dq1 -%}
	AND s._VALID_FG = 1
	{%- endif %};
